
@{
}

@model ProjectsViewModel

<h1>The book project</h1>


@foreach (Project p in Model.Projects)
            {
                <div class="card card-outline-primary m-1 p-1">
                    <div class="bg-faded p-1">
                        <h3>@p.Title</h3>
                        <ul>
                            <li>Author: @p.Author</li>
                            <li>Classification: @p.Classification</li>
                            <li>Category: @p.Category</li>
                            <li>Page Count: @p.PageCount</li>
                            <li>ISBN: @p.Isbn</li>
                            <li>Publisher: @p.Publisher</li>
                            <li>Price: @p.Price</li>
                        </ul>
                     </div>
                 </div>

                 <br />







                    }


                    <div page-blah="@Model.PageInfo" page-action="Index"></div>
